#include<stdio.h>

 int main(){

   int nota;
   int acumNota = 0;
   float promedioNotas;
   int cantAprobados = 0;
   
   
   
 for(int i = 1;i<=10;i++){
    printf (" El valor del indice i es : %d \n",i);
    printf (" Ingrese la nota del alunmo %d \n",i);
    scanf ("%d", &nota);


    if (nota >=6 ){

         acumNota = acumNota + nota ;
         cantAprobados = cantAprobados +1;
    }


}

    promedioNotas = acumNota/cantAprobados;

    printf ("El promedio de los que aprobaron es : %.1f\n",promedioNotas);


return 0;
}
